alias ll='ls -f -lah'
alias llt='ls -lahtcr'
alias sl='open -a "Sublime Text.app"'
alias vi=vim
alias mvi='mvim'
alias st='open -a SourceTree'
alias ij='open -a "IntelliJ IDEA 14 CE"'
